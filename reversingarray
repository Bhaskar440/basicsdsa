import java.util.Arrays;

public class tcs {
    public static void main(String[] args) {
        int arr[] = {5,4,3,2,1};
        int temp = 0;
        int start = 0;
        int end = arr.length-1;
        while(start<end)
        {
            temp=arr[start];
            arr[start]=arr[end];
            arr[end]=temp;
            start++;
            end--;
        }
        for(int i=0;i<=arr.length-1;i++)
        {
            System.out.println(arr[i]);
        }
    }
}
