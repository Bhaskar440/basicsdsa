//smallest element of array 
public class tcs {
    public static void main(String[] args)
    {
        int arr[] = {2,5,1,3,0};
        int small= arr[0] ;
        for(int i=0;i<arr.length;i++)
        {
            if(small>arr[i])
            {
                small=arr[i];
            }
        }
        System.out.println(small);
    }
}
