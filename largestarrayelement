..largest element of array
public class tcs {
    public static void main(String[] args)
    {
        int arr[] = {2,5,1,3,0};
        int large= arr[0] ;
        for(int i=0;i<arr.length;i++)
        {
            if(large<arr[i])
            {
                large=arr[i];
            }
        }
        System.out.println(large);
    }
}
